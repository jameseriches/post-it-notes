<h1>Post-It Notes</h1>
<%= link_to "New Note", new_page_path%>
<% @pages.each do |page| %>
  <h2><%= page.subject%></h2>
  <%# <a href='/'> %>
  <%= link_to 'Show Note', page_path(page)%>
  <%= link_to 'Edit Note', edit_page_path(page)%>
  <%= link_to 'Delete Note', page, method: :delete %>

  <%# <a href="/pages/1">Show Page</a> %>
<% end %>
